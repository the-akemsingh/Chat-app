{
    "version": 2,
    "builds": [
      {
        "src": "src/main.tsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "cache-control": "s-maxage=0"
        }
      }
    ],
    "env": {
      "VITE_WS_URL": "wss://backendca.akemnoorsingh.me"
    }
  }